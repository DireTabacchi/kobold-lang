var vals: array[3]float = { 1.0, 2.0, 3.0 };

proc mult_by_nums(vals: array[3]float) -> array[3]float {
    const my_arr: array[3]float = { 3.14159265, 1.61803398, 6.02214076 };
    var results: array[3]float;

    for var idx := 0; idx < len(vals); idx += 1 {
        results[idx] = vals[idx] * my_arr[idx];
    }

    return results;
}

var start: int;
var end: int;
start = clock();
var results: array[3]float;
for var i := 0; i < 10; i += 1 {
    results = mult_by_nums(vals);
}
end = clock();
println("results: { ", results[0], ", ", results[1], ", ", results[2], " }");

var delta := end - start;
println("start: ", start);
println("end: ", end);
println("time delta: ", delta, "ns");

