var greater_test := 7 - 2 > 3 + 4;          // eval to false
var lesser_test := 3 < 12 / 2;              // eval to true
var leq_test := 7 * 2 <= 19 - 6;            // eval to false
var geq_test := 3 >= 12 % 9;                // eval to true
var and_test := lesser_test && leq_test;    // eval to false
var or_test := leq_test || geq_test;        // eval to true
var eq_test := leq_test == and_test;        // eval to true
var neq_test := 123 != 234;                 // eval to true
var fneq_test := 12.3 != 12.3;              // eval to false

println(greater_test);
println(lesser_test);
println(leq_test);
println(geq_test);
println(and_test);
println(or_test);
println(eq_test);
println(neq_test);
println(fneq_test);
