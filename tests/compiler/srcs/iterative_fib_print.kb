// Fibonacci sequence iterative algorithm.
// Largest n is 92, 93 puts b past the signed integer limit.
var a := 0;
var b := 1;

// Set n to 1 to 92
var n := 10;

println("Computing Fibonacci sequence for n = ", n);

for var i := 1; i < n; i += 1 {
    print(b);
    var c := a + b;
    a = b;
    b = c;
    if i + 1 < n {
        print(", ");
    } else {
        print("\n");
    }
}

println("Result: ", b);
